include "table.robray"
include "lwa4d.robray"
include "robotiq.robray"


frame table_base {
    translation [0, 0, 0];
    rpy [0, 0, 0];
}

frame front_table {
    parent table_base;
    translation [1.125, 0, 0];
    geometry {
        isa table;
        dimension [100*inch, 30*inch, table_size];
    }
}

frame robot_podest_joint {
      parent front_table;
}