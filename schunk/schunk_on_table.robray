include "table.robray"
include "robotiq.robray"
include "lwa4d.robray"

def table_offset 1.125;

frame table_base {
    translation [0, 0, 0];
    rpy [0, 0, 0];
}

frame front_table {
    parent table_base;
    translation [table_offset, 0, 0];
    geometry {
        isa table;
        dimension [100*inch, 30*inch, table_size];
    }
}

frame robot_podest_joint {
      parent front_table;

}
