include "schunk_mounted.robray"
include "table.robray"

frame table_base {
    translation [20*inch,0, 0];
    rpy [0, 0, 0];
}

frame front_table {
    parent table_base;
    translation [1.125, 0, 28*inch];
    geometry {
        isa table;
        dimension [100*inch, 30*inch, table_size];
    }
}