frame robot_mount {
      type fixed;
      rpy [pi/2,0,pi/4];
      geometry {
      	       visual 0;
	       mesh "./mount.stl";
	       color [0.5,0.5,0.5];

	       alpha 1.0;
      }
      geometry {
		collision 0;
		mesh "./mount.stl";
		color [0.5, 0.5, 0.5];
		alpha 1.0;
	}
}

frame robot_podest_joint {
      	parent robot_mount;
	translation [.13, .875, .38];
	rpy [-pi/4, 0, 0];
}

include "lwa4d.robray"
include "robotiq.robray"