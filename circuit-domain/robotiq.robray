def mm 1e-3;
def cm 1e-2;
def inch 2.54*cm;
frame robotiq_gripper {
        parent robot_ee_joint;
	type fixed;
	rpy [pi/2, 0, pi/2];
	geometry {
		visual 0;
		mesh "./Robotiq-2F-85.stl";
		color [0.5, 0.5, 0.5];
		alpha 1.0;
	}
	geometry {
		visual 1;
		mesh "./Robotiq-2F-85.stl";
		color [0.5, 0.5, 0.5];
		alpha 1.0;
	}
}

/** Grasp **/
frame end_effector_grasp {
    parent robotiq_gripper;
    translation [0, 5.75*inch, 0];
    rpy [pi/2, 0, 0];
}